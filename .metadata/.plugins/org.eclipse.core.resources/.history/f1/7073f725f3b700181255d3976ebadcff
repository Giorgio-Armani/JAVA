
public class Tableau1D {

	public static void main(String[] args) {
		
		int entiersPremiers[]= {2,3,5,7,11,13,17,19,23};
		
		int tailleTableau = entiersPremiers.length;
		
		System.out.println("La taille du tableau : " + tailleTableau);
		
		System.out.println("Le premier element du tableau : " + entiersPremiers[0]);
		System.out.println("Le 2ème element du tableau : " + entiersPremiers[1]);
		System.out.println("Le 3ème element du tableau : " + entiersPremiers[2]);
		System.out.println("Le 4ème element du tableau : " + entiersPremiers[3]);
		System.out.println("Le 5ème element du tableau : " + entiersPremiers[4]);
		System.out.println("Le 6ème element du tableau : " + entiersPremiers[5]);
		System.out.println("Le 7ème element du tableau : " + entiersPremiers[6]);
		System.out.println("Le 8ème element du tableau : " + entiersPremiers[7]);
		System.out.println("Le 9ème element du tableau : " + entiersPremiers[8]);
		
		for (int i = 0; 1 < entiersPremiers.length; i++) {
			System.out.println(entiersPremiers[i]);
		}


		

	}

}
