public class IfElseIfEslseCondition {

	public static void main(String[] args) {
		//declaration
		int note;
		//affectation
		note = 10;
		
		if(note > 10) {
			System.out.println("La note est >= 10");
		}else if(note==10) {
			System.out.println("La note est = 10");
		}else{
			System.out.println("La note est < 10");
		}
		System.out.println("FIN");
		

	}

}
